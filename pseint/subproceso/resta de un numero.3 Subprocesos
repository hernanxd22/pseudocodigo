Algoritmo sin_titulo
	Definir num Como Entero;
	Escribir "ingresa un numero que se divida 2 veses ";
	Leer num;
	suma(num);
FinAlgoritmo

SubProceso suma(num)
	Definir num1,a,cont Como Entero;
	Escribir "ingrese el valor el cual se divida";
	Leer num1;
	Repetir
		num=num-num1;
		cont=cont+1;
		Escribir"la resta es ",num;
    Hasta Que num1>=num
	Escribir "el cociente es ",cont;
	Escribir "el residuo es ",num;
FinSubProceso
	