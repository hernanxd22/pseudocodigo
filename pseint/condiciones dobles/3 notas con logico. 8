Algoritmo sin_titulo
	Definir num,num1,num2 Como Entero;
	Definir blood Como Logico;
	Escribir "ingresa un numero para la nota 1";
	Leer num;
	Escribir "ingresa un numero para la nota 2";
	Leer num1;
	Escribir "ingresa un numero para la nota 3";
	Leer num2;
	Si num >= 1 y num <=10 Entonces
		blood=Verdadero;
		Escribir blood;
	SiNo
		blood=falso;
		Escribir blood;
	Fin Si
	Si num1 >= 1 y num1 <=10 Entonces
		blood=Verdadero;
		Escribir blood;
	SiNo
		blood=falso;
		Escribir blood;
	Fin Si
	Si num2 >= 1 y num2 <=10 Entonces
		blood=Verdadero;
		Escribir blood;
	SiNo
		blood=falso;
		Escribir blood;
	Fin Si
FinAlgoritmo
