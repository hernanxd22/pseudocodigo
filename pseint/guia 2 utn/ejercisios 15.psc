Algoritmo sin_titulo
		Definir matriz,i,j,num Como Entero;
		Dimension matriz[1,5];
		Para i<-0 Hasta 0 Hacer
			Para j<-0 Hasta 4 Hacer
				Escribir "ingresa 5 numeros ";
				Leer num;
				matriz[i,j]=num*num;
			Fin Para
		Fin Para
		
		Para i<-0 Hasta 0 Hacer
			Para j<-0 Hasta 4 Hacer
				Escribir Sin Saltar "[",matriz[i,j],"]";
			Fin Para
			Escribir ("");
		Fin Para
FinAlgoritmo

